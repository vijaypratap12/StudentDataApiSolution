@{
}


<form method="POST">
     <div>
        <label for="sid">Id</label>
        <input type="number" id="sid" name="Id" value="@Model.Id" readonly/>
    </div>
    <div>
        <label for="fname">FullName</label>
        <input type="text" id="fname" name="FullName" value="@Model.FullName" readonly/>
    </div>

    <div>
         <label for="email">Email</label>
        <input type="text" id="email" name="Email" value="@Model.Email" readonly/>
    </div>

    <div>
         <label for="address">Address</label>
        <input type="text" id="address" name="Address" value="@Model.Address" readonly/>
    </div>

    <div>
        <p class="text-danger">Are you sure you want to delete this?</p>
    </div>
    <div>
        <input type="submit" value="Submit" />
    </div>


</form>