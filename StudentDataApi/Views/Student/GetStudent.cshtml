@{
}


<table class = "table table-striped table-dark">
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th>Email</th>
        <th>Address</th>
        <th>Actions</th>
    </tr>

    @foreach(var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.FullName</td>
            <td>@item.Email</td>
            <td>@item.Address</td>
            <td>
                <a href = "/student/editstudent?id=@item.Id">Edit</a>
                <a href="/student/deletestudent?id=@item.Id">Delete</a>
            </td>

        </tr>
    }
</table>